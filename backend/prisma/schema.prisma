datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       Int      @id @default(autoincrement())
  name     String
  email    String   @unique
  password String
  bio      String?
  location String
  avatars  Avatar[]
}

model Avatar {
  id     Int    @id @default(autoincrement())
  url    Bytes
  userId Int
  user   User   @relation(fields: [userId], references: [id])
}